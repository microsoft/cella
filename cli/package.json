{
  "name": "vcpkg-ce.cli",
  "version": "0.4.0",
  "description": "vcpkg-ce CLI",
  "main": "dist/main.js",
  "directories": {
    "doc": "docs"
  },
  "engines": {
    "node": ">=10.12.0"
  },
  "scripts": {
    "eslint-fix": "eslint  . --fix --ext .ts",
    "eslint": "eslint  . --ext .ts",
    "clean": "shx rm -rf dist .rush *.log && shx echo Done",
    "build": "tsc -p .",
    "watch": "tsc -p . --watch",
    "prepare": "npm run build",
    "watch-test": "mocha dist/test --timeout 200000 --watch",
    "translate": "translate-strings ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Microsoft/vcpkg-ce.git"
  },
  "keywords": [
    "vcpkg-ce",
    "vcpkg",
    "ce"
  ],
  "author": "Microsoft",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Microsoft/vcpkg-ce/issues"
  },
  "homepage": "https://github.com/Microsoft/vcpkg-ce#readme",
  "readme": "https://github.com/Microsoft/vcpkg-ce/blob/master/readme.md",
  "devDependencies": {
    "@types/node": "14.14.22",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "eslint-plugin-notice": "0.9.10",
    "eslint": "^7.20.0",
    "@types/semver": "~7.3.4",
    "typescript": "4.3.2",
    "shx": "0.3.3",
    "translate-strings": "1.0.5",
    "@types/marked-terminal": "3.1.1",
    "@types/marked": "1.2.2",
    "@types/cli-progress": "3.9.1"
  },
  "dependencies": {
    "yaml": "2.0.0-5",
    "semver": "~7.3.4",
    "chalk": "4.1.0",
    "@microsoft/vcpkg-ce.core": "~0.4.0",
    "marked-terminal": "4.1.0",
    "marked": "2.0.0",
    "cli-progress": "3.9.0"
  }
}